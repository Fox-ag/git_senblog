  <!-- ここからプロフィール編集のカスタムはじまり -->
  <div class="section-12">
    
    <%= form_for current_user do |f| %>
    
    <div class="div-block-10">
      <div class="div-block-9">
        <h1>登録情報</h1>
      </div>
      <div class="div-block-14">
        <h3>プロフィール画像</h3>
        <div class="div-block-15">
          <%= f.file_field :image, class: "form-control floating-label", placeholder: "画像アップロード" %>
          <!--<h4>選択されていません</h4>-->
        </div>
      </div>
      <div>
        <h3>名前</h3>
        <div class="div-block-11">
          <h3>Last Name</h3>
          <h3 class="heading-20"><%= f.text_field :family_name, placeholder: "姓", class: "form-control" %></h3>
        </div>
        <div class="div-block-12">
          <h3>First Name</h3>
          <h3 class="heading-21"><%= f.text_field :first_name, placeholder: "名", class: "form-control" %></h3>
        </div>
      </div>
      <div>
        <h3>ニックネーム</h3>
        <div class="div-block-13">
          <h3><%= f.text_field :nickname, placeholder: "ニックネーム(必須)", class: "form-control" %></h3>
        </div>
      </div>
      <div>
        <h3>生年月日</h3>
        <div>
          <h3><%= f.text_field :birthdate, placeholder: "例) 2001年1月1日の場合、20010101", class: "form-control" %></h3>
        </div>
      </div>
      <div>
        <h3>国籍</h3>
        <div>
          <h3><%= f.text_field :country, placeholder: "お住いの国", class: "form-control" %></h3>
        </div>
      </div>
      <div>
        <h3>住んでいる町</h3>
        <div>
          <h3><%= f.text_field :city, placeholder: "お住いの市町村", class: "form-control" %></h3>
        </div>
      </div>
      <div class="div-block-16">
        <h3>自己紹介</h3>
        <div>
          <p class="paragraph-4">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Suspendisse varius enim in eros elementum tristique. Duis cursus, mi quis viverra ornare, eros dolor interdum nulla, ut commodo diam libero vitae erat. Aenean faucibus nibh et justo cursus id rutrum lorem imperdiet. Nunc ut sem vitae risus tristique posuere.</p>
        </div>
      </div>
      <div class="div-block-16">
        <h3><%= f.submit "送信", class: "btn btn-primary withripple" %></h3>
      </div>
    </div>
    
    <% end %>
  </div>
    <!-- ここまでプロフィール編集のカスタムおわり -->
  
  
